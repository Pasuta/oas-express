{
  "name": "oas-express",
  "version": "0.0.1",
  "private": true,
  "description": "OpenAPI Specification test",
  "keywords": [],
  "author": "",
  "license": "",
  "main": "src/app.js",
  "dependencies": {
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.3",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "express-openapi": "^2.0.4",
    "jest": "^22.4.4",
    "js-yaml": "^3.12.0",
    "should": "^7.1.1",
    "supertest": "^3.1.0",
    "swagger-ui-express": "^3.0.10"
  },
  "jest": {
    "testURL": "http://localhost",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(js)$",
    "moduleFileExtensions": [
      "js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ],
    "modulePaths": [
      "<rootDir>/src/"
    ],
    "silent": false,
    "collectCoverage": true
  },
  "scripts": {
    "start": "node ./src/app.js",
    "test": "jest"
  }
}
